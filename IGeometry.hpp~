#ifndef __IGEOMETRY_HPP
#define __IGEOMETRY_HPP

//interface untuk geometry
//benda bisa dirotasi
class IRotateable{
  virtual void rotate(float degree) = 0;
	virtual void rotate(float degree,int x0,int y0) = 0;
};

//benda bisa digerakkan
class IMoveable{
	virtual void move(int dx,int dy);
};

#endif

